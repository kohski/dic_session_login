<% if @blog.errors.any?%>
  <% @blog.errors.full_messages.each do |msg| %>
    <%= msg %>
  <% end %>
<% end %>
<%= form_with(model: @blog,local: true) do |form| %>
  <%= form.label :title%>
  <%= form.text_field :title %>
  <%= form.label :content%>
  <%= form.text_field :content %>
  <%= form.submit %>
<% end %>